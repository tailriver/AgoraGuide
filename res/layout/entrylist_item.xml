<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:paddingBottom="3dp"
	android:paddingTop="3dp" android:paddingLeft="5dp"
	android:paddingRight="5dp">
	<TextView android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/entrylist_item_title" android:layout_width="fill_parent"
		android:textColor="@android:color/primary_text_dark" android:textSize="17dp"
		android:singleLine="true" android:ellipsize="end"></TextView>
	<TextView android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/entrylist_item_sponsor" android:layout_width="fill_parent"
		android:textColor="@android:color/secondary_text_dark"
		android:textSize="12dp" android:singleLine="true" android:ellipsize="end"></TextView>
	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<TextView android:id="@+id/entrylist_item_schedule"
			android:text="TextView" android:layout_width="wrap_content"
			android:textColor="@android:color/secondary_text_dark"
			android:textSize="12dp" android:layout_height="fill_parent"
			android:layout_weight="1.0" android:singleLine="true"
			android:ellipsize="end"></TextView>
		<TextView android:id="@+id/entrylist_item_reservation"
			android:layout_width="30dp" android:textColor="@android:color/secondary_text_dark"
			android:layout_gravity="right" android:textSize="12dp"
			android:layout_height="fill_parent" android:text="@android:string/ok"></TextView>
		<TextView android:id="@+id/entrylist_item_category"
			android:text="TextView" android:layout_width="100dp"
			android:textColor="@android:color/secondary_text_dark"
			android:gravity="right" android:textSize="12dp"
			android:layout_height="fill_parent" android:singleLine="true"></TextView>
	</LinearLayout>
</LinearLayout>
