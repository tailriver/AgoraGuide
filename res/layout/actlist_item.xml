<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:paddingBottom="3dp"
	android:paddingTop="3dp" android:paddingLeft="5dp"
	android:paddingRight="5dp">
	<TextView android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/actlist_item_title" android:layout_width="fill_parent" android:textColor="@android:color/primary_text_dark" android:textSize="17dp"
		android:singleLine="true" android:ellipsize="end"></TextView>
	<TextView android:text="TextView" android:layout_height="wrap_content"
		android:id="@+id/actlist_item_exhibitor" android:layout_width="fill_parent" android:textColor="@android:color/secondary_text_dark" android:textSize="12dp"
		android:singleLine="true" android:ellipsize="end"></TextView>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<TextView android:id="@+id/actlist_item_schedule"
			android:text="TextView" android:layout_width="wrap_content" android:textColor="@android:color/secondary_text_dark" android:textSize="12dp"
			android:layout_height="wrap_content"></TextView>
	</LinearLayout>

</LinearLayout>
